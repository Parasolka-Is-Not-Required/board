/**
 * Библиотека ajax
 */

var req = Create(); 

function ge(id) 
{ 
    return document.getElementById(id); 
} 

function Create()
{  
    if(navigator.appName == "Microsoft Internet Explorer")
    {  
        req = new ActiveXObject("Microsoft.XMLHTTP");  
    }
    else
    {  
        req = new XMLHttpRequest();  
    }  
return req;
}

